      "notation": "c2br",
      "altlabel": null,
      "related": null,
      "@id": "c2br",
      "type": "termtype:ParcelPurpose",
      "@type": "skos:Concept"
    },
    {
      "preflabel": "Cross Lease Building",
      "definition": "A parcel created to enable a record of title comprising a leasehold estate and a share of the estate in fee simple to be issued",
      "notation": "clb",
      "altlabel": null,
      "related": null,
      "@id": "clb",
      "type": "termtype:ParcelPurpose",
      "@type": "skos:Concept"
    },
    {
      "preflabel": "Customary Entitlement",
