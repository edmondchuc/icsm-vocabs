cs:
    a skos:ConceptScheme ;
    owl:versionIRI :1.0 ;
    skos:definition "Affixes (prefixes, infixes, suffixes) of a conceptual nature (i.e. unrelated to a place's characteristics) which are applicable to geographical names, though typically not permitted for use in contemporary naming practices."@en ;
    skos:hasTopConcept
        :branch ,
        :central ,
        :deviation ,
        :east ,
        :extension ,
        :heights ,
        :inner ,
        :left ,
        :lower ,
        :north ,
        :north-east ,
        :north-west ,
        :outer ,
        :right ,
        :south ,
        :south-east ,
        :south-west ,
        :upper ,
        :west ;
    skos:historyNote """This vocabulary replaces and extends the Road Suffixes used in the Queensland Roads and Tracks dataset. 

2025-05 EC: Added SIR datatype codes""" ;
    skos:prefLabel "Geographical Name Affix"@en ;
    prov:qualifiedAttribution
        [
            dcat:hadRole isoroles:custodian ;
            prov:agent
                [
                    a schema:Person ;
                    schema:email "mailto:icsm@ga.gov.au"^^xsd:anyURI ;
                    schema:name "ICSM" ;
                ] ;
        ] ;
    schema:creator
        [
            a schema:Organization ;
            schema:name "Department Of Resources" ;
            schema:url "https://www.nrmmrrd.qld.gov.au/"^^xsd:anyURI ;
        ] ;
    schema:dateCreated "2024-06-11"^^xsd:date ;
    schema:dateModified "2025-05-22"^^xsd:date ;
    schema:keywords unggim:GeographicalNames ;
    schema:publisher
        [
            a schema:Organization ;
            schema:name "ICSM" ;
            schema:url "https://www.icsm.gov.au/"^^xsd:anyURI ;
        ] ;
