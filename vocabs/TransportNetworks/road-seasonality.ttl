:seasonal-closure
    a skos:Concept ;
    skos:altLabel
        "T"@en ,
        "True"@en ;
    skos:definition "True - subject to seasonal closure"@en ;
    skos:historyNote "Queensland Roads and Tracks dataset" ;
    skos:inScheme cs: ;
    skos:prefLabel "Seasonal Closure"@en ;
.

:unknown
    a skos:Concept ;
